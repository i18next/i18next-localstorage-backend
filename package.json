{
  "name": "i18next-localstorage-backend",
  "version": "1.1.4",
  "description": "caching layer backend for i18next using browsers localStorage",
  "main": "./index.js",
  "jsnext:main": "dist/es/index.js",
  "keywords": [
    "i18next",
    "i18next-backend"
  ],
  "homepage": "https://github.com/i18next/i18next-localStorage-backend",
  "bugs": "https://github.com/i18next/i18next-localStorage-backend/issues",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-preset-es2015-rollup": "3.0.0",
    "babel-preset-stage-0": "^6.22.0",
    "escope": "^3.6.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "mkdirp": "0.5.1",
    "rimraf": "2.6.1",
    "rollup": "^0.60.7",
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-node-resolve": "2.0.0",
    "rollup-plugin-uglify": "1.0.1",
    "yargs": "6.6.0"
  },
  "scripts": {
    "lint": "eslint ./src/* --fix",
    "clean": "rimraf dist && mkdirp dist",
    "copy": "cp ./dist/umd/i18nextLocalStorageBackend.min.js ./i18nextLocalStorageBackend.min.js && cp ./dist/umd/i18nextLocalStorageBackend.js ./i18nextLocalStorageBackend.js",
    "build:es": "BABEL_ENV=jsnext babel src --out-dir dist/es",
    "build:cjs": "babel src --out-dir dist/commonjs",
    "build:umd": "rollup -c rollup.config.js --format umd && UGLIFY=true rollup -c rollup.config.js --format umd",
    "build:amd": "rollup -c rollup.config.js --format amd && UGLIFY=true rollup -c rollup.config.js --format umd",
    "build:iife": "rollup -c rollup.config.js --format iife && UGLIFY=true rollup -c rollup.config.js --format iife",
    "build": "npm run clean && npm run build:cjs && npm run build:es && npm run build:umd && npm run copy",
    "preversion": "npm run build && git push",
    "postversion": "git push && git push --tags"
  },
  "author": "Jan MÃ¼hlemann <jan.muehlemann@gmail.com> (https://github.com/jamuhl)",
  "license": "MIT"
}
